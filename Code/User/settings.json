{
	"autoDarkMode.lightTheme": "GitHub Sharp Redux",
	"autoDarkMode.darkTheme": "lucy-evening",
	"breadcrumbs.enabled": true,
	"debug.chrome.useV3": true,
	"debug.console.fontSize": 14,
	"debug.inlineValues": true,
	"debug.node.useV3": true,
	"debug.toolBarLocation": "docked",
	"diffEditor.ignoreTrimWhitespace": true,
	"diffEditor.renderSideBySide": false,
	"editor.acceptSuggestionOnEnter": "off",
	"editor.find.globalFindClipboard": true,
	"editor.find.seedSearchStringFromSelection": false,
	"editor.fontFamily": "FiraCode-Retina, \"Fira Code Retina\", \"Fira Code\", \"SF Pro\"",
	"editor.fontLigatures": true,
	// "editor.fontSize": 13,
	"editor.fontSize": 14,
	"editor.insertSpaces": false,
	"editor.largeFileOptimizations": false,
	"editor.renderControlCharacters": true,
	"editor.renderIndentGuides": true,
	"editor.renderLineHighlight": "all",
	"editor.renderWhitespace": "all",
	"editor.rulers": [ 70, 100 ],
	"editor.smoothScrolling": true,
	"editor.suggest.localityBonus": true,
	"editor.suggestSelection": "recentlyUsedByPrefix",
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"eslint.nodePath": "~/.local",
	"explorer.confirmDragAndDrop": false,
	"extensions.ignoreRecommendations": false,
	"files.associations": {
		"*.control": "yaml",
		"*.h": "objective-c",
		"*.mk": "makefile",
		"*.plist": "xml",
		"*.strings": "xml",
		"*.Td": "makefile",
		"*.x": "objective-c",
		"*.xm": "objective-cpp",
		".prettierignore": "gitignore"
	},
	"files.autoSave": "onFocusChange",
	"files.insertFinalNewline": true,
	"git.autofetch": true,
	"git.fetchOnPull": true,
	"git.inputValidationLength": 80,
	"git.inputValidationSubjectLength": 70,
	"javascript.implicitProjectConfig.checkJs": true,
	"javascript.referencesCodeLens.enabled": true,
	"npm.enableScriptExplorer": true,
	"problems.showCurrentInStatus": true,
	"python.linting.enabledWithoutWorkspace": false,
	"remote.onstartup": true,
	"search.smartCase": true,
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.fontFamily": "FiraCode-Retina, \"Fira Code Retina\", \"Fira Code\", \"SourceCodePro+Powerline+Awesome Regular\", \"SF Pro Text\"",
	// "terminal.integrated.fontSize": 12,
	"terminal.integrated.fontSize": 14,
	"terraform.indexing": {
		"enabled": false,
		"liveIndexing": false,
		"delay": 500,
		"exclude": [
			".terraform/**/*",
			"**/.terraform/**/*"
		]
	},
	"terraform.languageServer": {
		"enabled": true,
		"args": []
	},
	"typescript.implementationsCodeLens.enabled": true,
	"typescript.referencesCodeLens.enabled": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"window.menuBarVisibility": "toggle",
	"window.zoomLevel": 0,
	"workbench.activityBar.visible": true,
	"workbench.colorTheme": "GitHub Sharp Redux",
	"workbench.editor.focusRecentEditorAfterClose": false,
	"workbench.editor.swipeToNavigate": true,
	"workbench.fontAliasing": "antialiased",
	"workbench.iconTheme": "vscode-great-icons",
	"workbench.list.horizontalScrolling": false,
	"workbench.sideBar.location": "left",
	"workbench.startupEditor": "newUntitledFile",
	"workbench.statusBar.feedback.visible": false,
	"workbench.statusBar.visible": true,
	"zenMode.hideStatusBar": false,
	"zenMode.hideTabs": false,
	"[diff]": {
		"editor.trimAutoWhitespace": false,
		"files.trimTrailingWhitespace": false
	},
	"[git-commit]": {
		"editor.rulers": [ 70, 80 ]
	},
	"[markdown]": {
		"editor.formatOnSave": true,
		"editor.renderWhitespace": "all"
	},
	"[yaml]": {
		"editor.insertSpaces": true
	}
}
